package classObjectAndMethod;

public class FirstClass {
	int x = 90;
	int y = 100;

	public static void main(String[] args) {

	}
	
	//void meaning this method will not return anything
	public void sum() {
		int a = 90;
		int b = 100;
		int c = a + b;
		
		System.out.println("Sum of two numbers are "+c);
	}

}
