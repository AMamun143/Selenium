package javaLessons;

public class IfElseStatement {
	/*
	 * if(boolean_expression)
	 * {
	 * 	execute if the boolean is true
	 * }
	 */

	public static void main(String[] args) {
		int x = 10;
		
		if (x == 20){
			System.out.println("Yes x == 10");
		}
		else{
			System.out.println("No x != 10");
		}

	}

}
